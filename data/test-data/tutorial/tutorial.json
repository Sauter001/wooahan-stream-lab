{
  "methods": [
    {
      "name": "findEvenNumbers",
      "description": "주어진 리스트에서 짝수만 걸러내세요",
      "testCases": [
        {
          "input": [1, 2, 3, 4, 5, 6],
          "expected": [2, 4, 6]
        },
        {
          "input": [34, 51, 28, 56, 99, 100],
          "expected": [34, 28, 56, 100]
        },
        {
          "input": [7, 8, 9, 10, 11, 12],
          "expected": [8, 10, 12]
        },
        {
          "input": [13, 15, 17, 18, 20],
          "expected": [18, 20]
        },
        {
          "input": [2, 4, 6, 8, 10, 12, 14],
          "expected": [2, 4, 6, 8, 10, 12, 14]
        },
        {
          "input": [99, 97, 95, 93],
          "expected": []
        },
        {
          "input": [5, 10, 15, 20, 25],
          "expected": [10, 20]
        },
        {
          "input": [0, 1, 2, 3, 4, 5],
          "expected": [0, 2, 4]
        },
        {
          "input": [101, 102, 103, 104],
          "expected": [102, 104]
        },
        {
          "input": [200, 201, 202],
          "expected": [200, 202]
        }
      ]
    }
  ]
}